<h1>q testcase: user/pass save, qbesas.dll</h1>


<script language=vbscript>
set iluser = CreateObject("QbeSAS.DataStore")

document.write "old user: '"+iluser.getUsername() + "'<br>"
document.write "old pass: '"+iluser.getPassword() + "'<br>"

stru = "user"
strp = "pass"
document.write "saving: '"+stru+"', '"+strp+"'<br>"
iluser.saveData "user", "pass", 1

strnu = iluser.getUsername()
strnp = iluser.getPassword()

document.write "new user: '"+strnu+ "'<br>"
document.write "new pass: '"+strnp+ "'<br>"

if (strnu = stru) then
	if (strnp = strp) then
	document.write "<b>Looks good, man!</b><br>"
	end if
end if

</script>

